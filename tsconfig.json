{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es2017",
            "es2015",
            "dom",
            "es6"
        ],
        "module": "commonjs",
        "rootDir": "./src",
        "outDir": "./dist",
        "sourceMap": true,
        "strict": true,
        "strictPropertyInitialization": false
    },
    "include": [
        "src/**/*.ts"
        ],
    "exclude": [
        "node_modules",
        "dist"
        ]
}